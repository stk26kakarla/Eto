
@{
    ViewBag.Title = "TimeZone List";
}

<h2>TimeZone List</h2>

@model string[]
<div style="height:80vh; margin:20px; padding:5px; border: 1px solid rgba(20,53,80,0.14); box-shadow: 0 1px 2px 1px rgba(0,0,0,.08), 0 3px 6px rgba(0,0,0,.08);overflow-y:scroll;overflow-x:hidden;">
    <ol>
        @foreach (var TimeZone in Model)
        {
            <li>@Html.Raw(TimeZone)</li>
        }
    </ol>
</div>